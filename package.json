{
  "name": "bettys",
  "version": "1.0.0",
  "description": "The web presence for Betty's Bar & Grill",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Wesley Lewis <wesley.l.lewis@gmail.com> (http://wesleylewis.us)",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "concurrently": "^3.6.1",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prepend-http": "^2.0.0",
    "validator": "^10.5.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  }
}
